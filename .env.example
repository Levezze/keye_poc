# Example environment variables for Keye POC
# Copy this file to .env and adjust values as needed.

# API security
API_KEY=dev-key

# Storage
STORAGE_BASE_PATH=storage
DATASETS_PATH=storage/datasets

# LLM (optional)
USE_LLM=true
LLM_PROVIDER=openai
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
LLM_MODEL=gpt-4.1-mini
LLM_TEMPERATURE=0.3
LLM_MAX_TOKENS=2000

# Server
DEBUG=false
RELOAD=true
ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:5173","http://localhost:8000"]
